.container {
    width: 100%;
    height: 65px;
    margin-bottom: 8px;

    display: flex;
    align-items: center;
    justify-content: center;

    font-size: 24px;
    color: var(--color-text-base);

    background: var(--color-header);
    border-radius: 4px;
    box-shadow: var(--shadow-default);
}